<BorderPane prefHeight="3170.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox prefHeight="574.0" prefWidth="477.0" style="-fx-background-color: #0B0C10; -fx-border-color: #1F2833;" BorderPane.alignment="CENTER">
         <padding>
            <Insets left="3.0" />
